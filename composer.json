{
    "name": "rpurinton/bunny",
    "type": "library",
    "description": "Performant pure-PHP AMQP (RabbitMQ) sync/async (ReactPHP) library",
    "keywords": [
        "rabbitmq",
        "rabbit",
        "bunny",
        "amqp",
        "messaging",
        "message",
        "queue",
        "queueing",
        "exchange",
        "react",
        "react-php",
        "reactphp"
    ],
    "minimum-stability": "stable",
    "version": "1.0.4",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Kulhan",
            "email": "jakub.kulhan@gmail.com"
        },
        {
            "name": "Russell Purinton",
            "email": "russell.purinton@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "react/event-loop": "^1.0 || ^0.5 || ^0.4",
        "react/promise": "^3.0 || ^2.2",
        "rpurinton/config": "^1.0"
    },
    "suggest": {
        "ext-pcntl": "For using synchronous AMQP/RabbitMQ client"
    },
    "autoload": {
        "psr-4": {
            "RPurinton\\Bunny\\": "src/"
        }
    }
}